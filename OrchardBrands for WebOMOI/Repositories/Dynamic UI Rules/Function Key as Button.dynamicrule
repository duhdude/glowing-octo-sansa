<rule>
  <matches>
  	<match>
      <container id="button" template="Button">
        <field id="title" min_occurrence="1" max_occurrence="1">
          <attributes>
            <editable>false</editable>
          </attributes>
          <content>
          	<!--  pattern is F1=HELP -->
            <regexp pattern="(?&lt;key&gt;F\d+)=(?&lt;label&gt;([^ ]+( [^F ]+)*)+)"/>
          </content>
        </field>
      </container>
    </match>
  </matches>
</rule>
