<rule>
  <matches>
  	<match>
      <container id="menu_button" template="Menu Button">
        <field id="row" min_occurrence="2" search_direction="vertical" skip_empty="true">
          <attributes>
            <editable>false</editable>
          </attributes>
          <content>
            <regexp pattern="^(?&lt;send_string&gt;\d+)\. *(?&lt;label&gt;.+)"/>
          </content>
        </field>
      </container>
    </match>
  </matches>
</rule>
